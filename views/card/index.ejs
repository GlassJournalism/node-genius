<div id="container" class="container clearfix">
    <% for(var i = 0; i < cards.length; i++){ %>
    <div class="small-12 medium-12 large-9 xlarge-6 columns card-item">
        <div class="panel">
            <div><h1><%= cards[i].name %></h1></div>
            <iframe class="glass-preview" src="/card/preview/<%= cards[i].id %>"></iframe>
        </div>
    </div>
    <% } %>
</div>